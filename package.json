{
    "name": "react-mega-news",
    "version": "0.0.1",
    "description": "React + Node MegaNews",
    "main": "client/index.js",
    "scripts": {
        "test": "Error testing: Nocommand initialised",
        "client": "cd client && npm start",
        "server": "node server/Server.js",
        "create": "sequelize db:drop || sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
        "dev": "npm run create && concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kirban/react-mega-news"
    },
    "keywords": [
        "react",
        "node",
        "news",
        "meganews"
    ],
    "author": "Kirban",
    "license": "ISC",
    "dependencies": {
        "express": "^4.16.3",
        "mysql": "^2.15.0",
        "mysql2": "^1.5.3",
        "redux-actions": "^2.6.1",
        "sequelize": "^4.38.0"
    },
    "devDependencies": {
        "axios": "^0.18.0",
        "concurrently": "^3.5.0",
        "sequelize-cli": "^4.0.0"
    }
}
